<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Bark N Bite - Admin</title>
    <link rel="stylesheet" href="/css/uikit.css">
    <script src="/js/uikit.min.js"></script>
    <script src="/js/uikit-icons.min.js"></script>
</head>

<body>
    <div style="padding:40px;">




        <%- messages('message', locals) %>
        <a href="/admin/xyz/sandeep/barknbite/@1234/5678">Back To Menu</a>
        <br>
        <div class="uk-overflow-auto">
            <table class="uk-table uk-table-hover uk-table-middle uk-table-divider">
                <thead>
                    <tr>
                        <th class="uk-table-shrink">No.</th>

                        <th class="uk-table-expand">Order Id</th>
                        <th class="">Email/ Phone</th>
                        <th class="uk-table-shrink uk-text-nowrap">Date</th>
                        <th class="uk-table-shrink">Check</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(var i=0; i < allor.length; i++) { %>
                    <tr>
                        <td>
                            <%= i+1 %>
                        </td>

                        <td class="uk-table-link">
                            <a class="uk-link-reset" href="/admin/xyz/sandeep/barknbite/@1234/5678/orders/edit/<%= allor[i]._id %>">
                                <%= allor[i].oid %></a>
                        </td>
                        <td class="uk-text-nowrap">
                            <%= allor[i].email %> &nbsp;&nbsp;
                            <%= allor[i].pno %>
                        </td>
                        <td class="uk-text-nowrap">
                            <%= allor[i].date %>
                        </td>
                        <td>
                            <a href="/admin/xyz/sandeep/barknbite/@1234/5678/orders/check/<%= allor[i]._id %>" uk-icon="check"></a>
                        </td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>





    </div>

</body>

</html>